  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        View Students
        <small> </small>
      </h1>
      
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          

          <div class="box">
            <div class="box-header">
              <h3 class="box-title">A list of all Students</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th></th>
                  <th>Student's Name</th>
                  <th>Registration Number</th>
                  <th>Programme</th>
                  <th>Student Type</th>
                  <th>Year of Study</th>
                  <th>Semester</th>
                </tr>
                </thead>
                <tbody>
                <% var count = 1%>
                <% for (var row in students){ %>
                <tr>
                  <td style="font-weight: bold;"><%= count %>.</td>
                  <td><%= students[row].first_name %> <%= students[row].last_name %> <%= students[row].middle_name %> <%= students[row].maiden_name %></td>
                  <td><%= students[row].regno %></td>
                  <td><%= students[row].programme_code %></td>
                  <td><%= students[row].student_type %></td>
                  <td><%= students[row].year_of_study %></td>
                  <td><%= students[row].semester %></td>
                </tr>

                <% count = count + 1 %>
                <% } %>
                
                </tbody>
                <tfoot>
                <tr>
                  <th></th>
                  <th>Student's Name</th>
                  <th>Registration Number</th>
                  <th>Programme</th>
                  <th>Student Type</th>
                  <th>Year of Study</th>
                  <th>Semester</th>
                </tr>
                </tfoot>
              </table>
              <tr>
                <td colspan="4" align="right"><a href="/new_student" role="button" class="btn btn-success">Register New Student </a></td>
              </tr>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>

  <!-- /.content-wrapper -->

