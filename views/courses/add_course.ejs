    <!-- Main content -->
    <section class="content">
      <div class="row">
        <!-- left column -->
        
        <!--/.col (left) -->
        <!-- right column -->
        <div class="col-md-12">
          <!-- Horizontal Form -->
          <div class="box box-info">
            <div class="box-header with-border">
              <h3 class="box-title">    </h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form class="form-horizontal" method="POST" action="/course/add" >
              <div class="box-body">
                <div class="form-group">
                  <label for="name" class="col-sm-2 control-label">Name</label>

                  <div class="col-sm-5">
                    <input  class="form-control" name="name" placeholder="Course Name">
                  </div>
                </div>
                <div class="form-group">
                  <label for="code" class="col-sm-2 control-label">Code</label>

                  <div class="col-sm-5">
                    <input  class="form-control" name="code" placeholder="Course code">
                  </div>
                </div>

                <div class="form-group">
                  <label for="department" class="col-sm-2 control-label">Department</label>

                  <div class="col-sm-5">
                      <select name="department" id="department">

                            <% departments.forEach(function(department) { %>

                                            <option value="<%= department.department_id %>"><%= department.department_name %></option>

                              <% }); %>
                        
                      </select>
                  </div>
                </div>

                <div class="form-group">
                  <label for="year_offered" class="col-sm-2 control-label">Year Offered</label>

                 <div class="col-sm-5">
                      <select name="year_offered">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                      </select>
                  </div>
                </div>

                <div class="form-group">
                  <label for="semester_offered" class="col-sm-2 control-label">Semester Offered</label>

                  <div class="col-sm-5">
                      <select name="semester_offered">
                            <option value="1">1</option>
                            <option value="2">2</option>
                      </select>
                  </div>
                </div>

                <div class="form-group">
                  <label for="lecture_hours" class="col-sm-2 control-label">Lecture Hours per week</label>

                  <div class="col-sm-5">
                    <input type="lecture" class="form-control" name="lecture_hours" type="number" placeholder="Lecture Hours">
                  </div>
                </div>

               <div class="form-group">
                  <label for="lab_hours" class="col-sm-2 control-label">Lab Hours per week</label>

                  <div class="col-sm-5">
                    <input type="lecture" class="form-control" name="lab_hours" type="number" placeholder="Lab Hours">
                  </div>
                </div>
                
              </div>
              <!-- /.box-body -->
              <div class="box-footer">
                <button type="submit" class="btn btn-default">Cancel</button>
                <button type="submit" class="btn btn-info pull-right">Submit</button>
              </div>
              <!-- /.box-footer -->
            </form>
          </div>
          <!-- /.box -->
          
