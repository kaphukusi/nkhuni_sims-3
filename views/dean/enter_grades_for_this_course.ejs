      <h1>
       <%= student.course_code %>  <%= students.course_name %>: Enter Grades
      </h1>
      
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <!-- left column -->
        
        <!--/.col (left) -->
        <!-- right column -->
        <div class="col-md-12">
          <!-- Horizontal Form -->
          <div class="box box-info">
            <div class="box-header with-border">
              <h3 class="box-title">   </h3>
            </div>
            <!-- /.box-header -->
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th></th>
                  <th>Student Reg No</th>
                  <th>Full name</th>
                  <th>Year of Study</th>
                  <th>Final Aggregate Grade</th>
                </tr>
                </thead>
                <tbody>
                <% var count = 1%>
                <% for (var row in students){ %>
                <tr>
                  <td style="font-weight: bold;"><%= count %>.</td>
                  
                  <td> <input type="hidden" name="regno" value="<%= students[row].regno %>"> <%= students[row].regno %> </td>
                  
                  <td> <input type="hidden" name="name" value="<%= students[row].first_name %> <%= students[row].middle_name %> <%= students[row].last_name %> <%= students[row].maiden_name %>"> <%= students[row].first_name %> <%= students[row].middle_name %> <%= students[row].last_name %> <%= students[row].maiden_name %> </td>

                  <td><input type="hidden" name="year_of_study" value="<%= students[row].year_of_study %>"><%= students[row].year_of_study %></td>
                  
                  <td> <input type="text" name="course_final_grade" onfocus="blue(this)" onblur="saveGrade('<%= student.course_code %>','2017','<%= students[row].year_of_study %>','<%= student.semester %>','<%= students[row].regno %>',this)"> </td>

                </tr>
                <input type="hidden" name="programme_id" value="<%= students[row].programme_id %>">
                
                <% count = count + 1 %>
                <% } %>
                
                <input type="hidden" name="semester" value="<%= student.semester %>">
                <input type="hidden" name="course_code" value="<%= student.course_code %>">
                
                </tbody>
                <tfoot>
                <tr>
                  <th></th>
                  <th>Student Reg No</th>
                  <th>Full name</th>
                  <th>Year of Study</th>
                  <th>Aggregate</th>
                </tr>
                </tfoot>
              </table>

