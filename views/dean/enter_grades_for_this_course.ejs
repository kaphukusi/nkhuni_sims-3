<!-- Page script -->


<script>
  function orange(element){
      element.style.color = "orange"
  }
  function blue(element){
      element.style.color = "blue"
  }
  function green(element){
      element.style.color = "green"
  }
  function saveGrade(course_code,academic_year,year_of_study,semester,regno,element){
    var url = "/dean/save_grades"
    var data = {
                  year_of_study: year_of_study,
                  course_code : course_code,
                  regno : regno ,
                  course_final_grade: element.value,
                  semester : semester,
                  academic_year : academic_year
                }
    console.log(data);
    $.post(url,data,function(data){
       if(data){
            green(element);
       }
    });
  }
 
</script>


